{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["es2016", "dom"],
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "outDir": "src-js",
    "noImplicitAny": false,
    "removeComments": false,
    "suppressImplicitAnyIndexErrors": false
  },
  "include": [
    "./src-ts/**/*.ts"
  ]
}
