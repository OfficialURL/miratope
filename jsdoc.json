{
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["./package.json", "./readme.md", "./src-js/"],
        "includePattern": "\\.js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
		"destination": "./out-jsdoc/",
		"tutorials": "./tutorials/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
	"docdash": {
		"sort": true,
		"private": true,
		"search": true,
		"collapse": true,
        "menu":{
            "Github repo":{
                "href":"https://github.com/officialurl/miratope",
                "target":"_blank",
                "class":"menu-item",
                "id":"repository"
            }
        }
	}
}
