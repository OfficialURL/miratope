{
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["package.json", "../readme.md", "../src/"],
        "includePattern": "\\.js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "docdash",
        "encoding": "utf8",
		"destination": "../out/",
		"tutorials": "../tutorials/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
	"docdash": {
		"sort": true,
		"private": true,
		"search": true,
		"collapse": true
	}
}
